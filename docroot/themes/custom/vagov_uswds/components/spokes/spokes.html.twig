<div class="usa-grid vads-u-padding-top--6">
	<div class="va-h-ruled--stars"></div>
</div>
<h2>{{spoke.field_title.value}}</h2>
{% for childParagraph in spoke.field_va_paragraphs %}
	{{ drupal_entity('paragraph', childParagraph.target_id) }}
{% endfor %}

