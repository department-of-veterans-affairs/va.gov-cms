<?php

/**
 * @file
 * Install, update and uninstall functions for the VA Gov Views module.
 */

/**
 * Implements hook_install().
 */
function va_gov_views_install() {
  \Drupal::messenger()->addStatus(__FUNCTION__);
}

/**
 * Implements hook_uninstall().
 */
function va_gov_views_uninstall() {
  \Drupal::messenger()->addStatus(__FUNCTION__);
}
