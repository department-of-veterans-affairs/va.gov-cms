# Ignore binaries.
/bin

# Ignore non-custom parts of Drupal.
/docroot/core
/docroot/libraries
/docroot/modules/contrib
/docroot/profiles/contrib
/docroot/themes/contrib
/docroot/sites/default/files
/docroot/sites/simpletest

# Ignore Composer-managed dependencies.
/docroot/vendor/

# Ignore NPM managed dependencies
node_modules/

# Ignore local dev files
/docroot/sites/default/settings/settings.local.php

# Ignore SQL dumps
# Current
/.dumps/cms-prod-files-latest.tgz
/.dumps/cms-prod-db-sanitized-latest.sql*
# Deprecated
/.dumps/cms-app-files-latest.tgz
/.dumps/cms-db-latest.sql*

# Ignore IntelliJ/PhpStorm
.idea

# Ignore CMS-CI files
/docroot/sites/sites.php
/docroot/sites/*/files
