# This value is used by the front-end to load Drupal content. 
# Copy this file to .env to make the values available to Composer and others.
# The standard lando address for local development.

# Used
SITE_URI="va-gov-cms.lndo.site"

# Used by WEB build process.
DRUPAL_ADDRESS=http://$SITE_URI

SITE_URL="http://$SITE_URI"
SITE_ALIAS="@self"
SITE_ROOT="/app/docroot"
BEHAT_PARAMS="{'extensions':{'Behat\\\\MinkExtension':{'base_url':'$SITE_URL'},'Drupal\\\\DrupalExtension':{'drush':{'root':'$SITE_ROOT','alias':'$SITE_ALIAS'},'drupal':{'drupal_root':'$SITE_ROOT'}}}}"

PATH="$PATH:/app/bin"