CMS_ENVIRONMENT_TYPE="tugboat"
CYPRESS_BASE_URL="{{ TUGBOAT_DEFAULT_SERVICE_URL }}"
# Disable shared memory usage for cypress electron browser.
ELECTRON_EXTRA_LAUNCH_ARGS='disable-dev-shm-usage'
SIMPLETEST_BASE_URL="{{ TUGBOAT_DEFAULT_SERVICE_URL }}"
DTT_BASE_URL="{{ TUGBOAT_DEFAULT_SERVICE_URL }}"
DRUPAL_ADDRESS="{{ TUGBOAT_DEFAULT_SERVICE_URL_PROTOCOL }}://{{ TUGBOAT_DEFAULT_SERVICE_URL_HOST }}"
FRONTEND_ADDRESS="{{ TUGBOAT_DEFAULT_SERVICE_URL_PROTOCOL }}://web-{{ TUGBOAT_DEFAULT_SERVICE_TOKEN }}.{{ TUGBOAT_SERVICE_CONFIG_DOMAIN }}"
GITHUB_TOKEN="{{ GITHUB_TOKEN }}"
NODE_EXTRA_CA_CERTS="{{ NODE_EXTRA_CA_CERTS }}"
CMS_JENKINS_VA_CMS_BOT_TOKEN="{{ CMS_JENKINS_VA_CMS_BOT_TOKEN }}"
CMS_FACILITY_API_KEY="{{ CMS_FACILITY_API_KEY }}"
CMS_FACILITY_API_URL="{{ CMS_FACILITY_API_URL }}"
CMS_VAGOV_API_KEY="{{ CMS_VAGOV_API_KEY }}"
CMS_VAGOV_API_URL="{{ CMS_VAGOV_API_URL }}"
CMS_GOVDELIVERY_ENDPOINT="{{ CMS_GOVDELIVERY_ENDPOINT }}"
CMS_GOVDELIVERY_USERNAME="{{ CMS_GOVDELIVERY_USERNAME }}"
CMS_GOVDELIVERY_PASSWORD="{{ CMS_GOVDELIVERY_PASSWORD }}"
BEHAT_PARAMS='{"extensions":{"Behat\\MinkExtension":{"base_url":"{{ TUGBOAT_DEFAULT_SERVICE_URL_PROTOCOL }}://{{ TUGBOAT_DEFAULT_SERVICE_URL_HOST }}"},"Drupal\\DrupalExtension":{"drush":{"root":"{{ TUGBOAT_ROOT }}/docroot","alias":"@none"},"drupal":{"drupal_root":"{{ TUGBOAT_ROOT }}/docroot"}}}}'

# GitHub-Commenter and GitHub-Status-Updater environment variables.
GITHUB_OWNER={{ TUGBOAT_GITHUB_OWNER }}
GITHUB_REPO={{ TUGBOAT_GITHUB_REPO }}
GITHUB_COMMENT_TYPE=pr
GITHUB_PR_ISSUE_NUMBER={{ TUGBOAT_GITHUB_PR | default('') }}
GITHUB_REF={{ TUGBOAT_PREVIEW_SHA }}
